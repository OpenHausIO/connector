{
  "name": "connector",
  "version": "1.1.0",
  "description": "Connector between backend &amp; local network",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "test": "echo \"No test specified\" && exit 0",
    "dev": "./node_modules/.bin/nodemon -w . -w .env index.js",
    "lint": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint --fix .",
    "build": "npx pkg .",
    "bundle": "grunt compress",
    "build:docker-image": "docker build . -t openhaus/connector:latest",
    "postinstall": "scripts/post-install.sh"
  },
  "pkg": {
    "scripts": [
      "http",
      "https"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-windows-x64",
      "node16-darwin-x64"
    ],
    "outputPath": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenHausIO/connector.git"
  },
  "author": "Marc Stirner <marc.stirner@open-haus.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OpenHausIO/connector/issues"
  },
  "homepage": "https://github.com/OpenHausIO/connector#readme",
  "dependencies": {
    "dotenv": "^16.0.0",
    "minimist": "^1.2.6",
    "ws": "^8.5.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "grunt": "^1.5.3",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-env": "^1.0.1",
    "nodemon": "^2.0.15",
    "pkg": "^5.8.0"
  }
}